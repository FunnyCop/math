{
    "tags": {
        "allowUnknownTags": ["optional"]
    },
    "plugins": [
        "node_modules/better-docs/typescript",
        "plugins/markdown"
    ],
    "applicationName": "Math",
    "linenums": true,
    "source": {
        "include": [
            "./util",
            "./pages"
        ],
        "includePattern": "\\.(jsx|js|ts|tsx)$"
    },
    "opts": {
        "encoding": "utf8",
        "recurse": true,
        "verbose": true,
        "private": false,
        "lenient": true,
        "destination": "./docs/",
        "template": "./node_modules/clean-jsdoc-theme",
        "theme_opts": {
            "theme": "dark"
        }
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    }
}